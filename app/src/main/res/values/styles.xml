<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="textBody">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">?android:textColorSecondary</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="textBody_lw0dp" parent="textBody">
        <item name="android:layout_width">0dp</item>
    </style>


    <style name="textBody.Small" parent="textBody">
        <item name="android:textSize">12sp</item>
    </style>


    <style name="textBody.VerySmall" parent="textBody">
        <item name="android:textSize">10sp</item>
    </style>

    <style name="textBody.VerySmall.Error" parent="textBody.VerySmall">
        <item name="android:textColor">?colorError</item>
    </style>

    <style name="textBody.Large" parent="textBody">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="textBody.VeryLarge" parent="textBody">
        <item name="android:textSize">18sp</item>
    </style>


    <style name="textHeading" parent="textBody">
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">?android:textColor</item>
    </style>

    <style name="textHeading.medium" parent="textHeading">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="textHeading.Small" parent="textHeading">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="textHeading.VerySmall" parent="textHeading">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="textHeading.NormalStyle" parent="textHeading.Small">
        <item name="android:textStyle">normal</item>
    </style>

    <style name="textHeading.NormalStyle.Large" parent="textHeading.medium">
        <item name="android:textStyle">normal</item>
    </style>

    <style name="textHeading.NormalStyle.Small" parent="textHeading.NormalStyle">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="textHeading.NormalStyle.VerySmall" parent="textHeading.NormalStyle">
        <item name="android:textSize">10sp</item>
    </style>

    <style name="textHeading.Large" parent="textHeading">
        <item name="android:textSize">22sp</item>
    </style>


    <style name="btn_lwm" parent="Widget.Material3.Button">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">52dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">?backgroundColor</item>
        <item name="backgroundTint">?colorPrimaryInverse</item>
        <item name="android:textAllCaps">false</item>
        <item name="cornerRadius">@dimen/buttonRadius</item>
        <item name="android:textSize">16sp</item>
        <item name="android:insetTop">0sp</item>
        <item name="android:insetBottom">0sp</item>
        <item name="iconTint">?backgroundColor</item>
    </style>


    <style name="btn_lwm_enable_disable" parent="btn_lwm">
        <item name="backgroundTint">@color/button_bg</item>
    </style>

    <style name="btn_lwm.textBtn" parent="Widget.Material3.Button.TextButton">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:paddingHorizontal" tools:targetApi="o">0dp</item>
        <item name="android:textSize">14sp</item>
        <item name="cornerRadius">@dimen/buttonRadius</item>
    </style>

    <style name="btn_lwm.outlined" parent="Widget.Material3.Button.OutlinedButton">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:minHeight">58dp</item>
        <item name="android:lines">1</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">?android:textColorSecondary</item>
        <item name="iconTint">?android:textColorSecondary</item>
        <item name="android:textAllCaps">false</item>
        <item name="cornerRadius">@dimen/buttonRadius</item>
        <item name="strokeColor">?colorButtonNormal</item>
    </style>

    <style name="btn_lw0dp.outlined" parent="Widget.Material3.Button.OutlinedButton">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:minHeight">55dp</item>
        <item name="android:lines">1</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?android:textColor</item>
        <item name="iconGravity">end</item>
        <item name="strokeWidth">1dp</item>
        <item name="android:paddingHorizontal" tools:targetApi="o">10dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="cornerRadius">@dimen/buttonRadius</item>
    </style>

    <style name="textInputLayout">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="boxBackgroundColor">?backgroundColor</item>
        <item name="boxCornerRadiusBottomEnd">@dimen/buttonRadius</item>
        <item name="boxCornerRadiusBottomStart">@dimen/buttonRadius</item>
        <item name="boxCornerRadiusTopEnd">@dimen/buttonRadius</item>
        <item name="boxCornerRadiusTopStart">@dimen/buttonRadius</item>
        <item name="boxStrokeColor">@color/stroke_color_with_effects</item>
        <item name="boxStrokeWidth">@dimen/et_stroke_width</item>
        <item name="hintEnabled">true</item>
        <item name="errorEnabled">true</item>
        <item name="boxStrokeErrorColor">?colorError</item>
        <item name="errorTextAppearance">@style/textBody.VerySmall.Error</item>
        <item name="expandedHintEnabled">true</item>
        <item name="hintTextColor">?colorPrimary</item>
    </style>


    <style name="textInputLayout.NoHint" parent="textInputLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="hintEnabled">false</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="expandedHintEnabled">false</item>
    </style>


    <style name="editTextInputLayout" parent="textBody">
        <item name="android:layout_width">match_parent</item>
        <item name="android:maxLength">80</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">10dp</item>
    </style>

    <style name="backBtn">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginTop">15dp</item>
        <item name="contentPadding">5dp</item>
        <item name="strokeWidth">0dp</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:background">?selectableItemBackground</item>
        <item name="android:src">@drawable/arrow_small_left</item>
        <item name="shapeAppearance">@style/ShapeAppearance.Material3.Corner.Full</item>
    </style>

    <style name="llSet">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">5dp</item>
        <item name="android:layout_marginTop">15dp</item>
    </style>

    <style name="TabTextAppearance" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">?fontFamily</item>
        <item name="fontFamily">?fontFamily</item>

    </style>

    <style name="SettingsButton" parent="Base.Theme.NETWORKOFONE">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:maxLines">2</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:background">?selectableItemBackground</item>
        <!--        <item name="endIcon">@drawable/angle_small_right_1_</item>-->

    </style>
    <!--        <item name="textColor">?android:textColor</item>-->


    <style name="linearProg">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:max">100</item>
        <item name="android:progressTint">@color/textColorSecondary</item>
        <item name="android:secondaryProgress">?colorPrimary</item>
        <item name="android:progress">0</item>
        <item name="trackColor">?colorTertiary</item>
        <item name="indicatorTrackGapSize">0dp</item>
        <item name="trackThickness">2dp</item>
        <item name="trackStopIndicatorSize">0dp</item>
        <item name="indicatorColor">?colorPrimary</item>
    </style>


    <style name="checkBoxStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingStart">5dp</item>
    </style>


    <style name="constraintLay" parent="constLay">
        <item name="android:id">@id/parent</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="selectableIcon" parent="backBtn">
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_width">40dp</item>
        <item name="android:padding">0dp</item>
        <item name="contentPadding">10dp</item>
        <item name="android:layout_marginTop">0dp</item>
    </style>


