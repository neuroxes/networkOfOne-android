    <style name="horizontal_divider">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginTop">15dp</item>
        <item name="android:background">?colorButtonNormal</item>
    </style>

    <style name="setting_item" parent="textHeading.NormalStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:paddingVertical" tools:targetApi="o">16dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingEnd">15dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:drawablePadding">15dp</item>
        <item name="android:paddingStart">20dp</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:drawableEnd">@drawable/angle_small_right</item>
        <item name="android:background">@drawable/ripple_effect</item>
    </style>

    <style name="radioBtn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/radio_btn_outlined_bg</item>
        <item name="android:layout_marginTop">15dp</item>
        <item name="android:paddingTop">17dp</item>
        <item name="android:paddingStart">3dp</item>
        <item name="android:lines">2</item>
        <item name="android:paddingBottom">17dp</item>
    </style>

    <style name="error_text_appearance">
        <item name="android:textSize">8sp</item>
    </style>

    <style name="circular_progress_indicator">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:indeterminate">true</item>
        <item name="android:visibility">gone</item>
        <item name="indicatorColor">?backgroundColor</item>
        <item name="indicatorSize">30dp</item>
        <item name="trackColor">?backgroundColor</item>
    </style>

    <style name="btn">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">52dp</item>
        <item name="android:layout_weight">1</item>
        <!--        <item name="android:background">@drawable/selectable_bg</item>-->
        <item name="android:textColor">?colorOnBackground</item>

    </style>

    <style name="editTextOutlined">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingHorizontal" tools:targetApi="o">10dp</item>
        <item name="android:paddingVertical" tools:targetApi="o">15dp</item>
        <item name="android:drawablePadding">15dp</item>
        <item name="android:textColorHint">#aaaaaa</item>
        <item name="android:background">@drawable/outlined_10dp_round_with_states_effect</item>
    </style>


    <style name="chipStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="chipCornerRadius">26dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/chip_text_color</item>
        <item name="chipBackgroundColor">@color/chip_background_color</item>
        <item name="android:checkable">true</item>
        <item name="chipIconTint">?colorOnBackground</item>

    </style>

    <style name="chipStyle2" parent="chipStyle">
        <item name="chipStrokeWidth">0dp</item>
        <item name="android:textColor">@color/chip_text_color2</item>
        <item name="chipBackgroundColor">@color/chip_background_color2</item>
    </style>

    <style name="constLay">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:background">?attr/selectableItemBackground</item>

    </style>

    <style name="viewAllSt" parent="btn_lwm.textBtn">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:drawablePadding">0dp</item>
        <item name="icon">@drawable/angle_small_right</item>
        <item name="android:padding">0dp</item>
        <item name="android:text">See all</item>
        <item name="android:gravity">center</item>
        <item name="iconGravity">textEnd</item>
    </style>

    <style name="tvSettings" parent="textBody">
        <item name="android:layout_width">0dp</item>
        <item name="android:background">@drawable/round_full_bg_with_ripple_effects</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:drawablePadding">15dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">15dp</item>

    </style>

    <style name="roundCard">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@drawable/round_bg</item>
        <item name="android:backgroundTint">@color/cardBackground</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingHorizontal" tools:ignore="NewApi">10dp</item>
        <item name="android:paddingVertical" tools:ignore="NewApi">15dp</item>
    </style>

    <style name="editTextOutlined_round">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="loading_anim">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="lottie_rawRes">@raw/incity_loading_anim</item>
        <item name="lottie_loop">true</item>
        <item name="lottie_autoPlay">true</item>

    </style>


    <style name="materialCard">
        <item name="android:id">@id/parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="strokeWidth">0dp</item>
        <item name="cardBackgroundColor">?backgroundColor</item>
        <item name="cardCornerRadius">@dimen/buttonRadius</item>
        <item name="android:pointerIcon">hand</item>


    </style>

    <style name="SettingsButton2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:maxLines">2</item>
        <item name="android:layout_marginHorizontal" tools:targetApi="26">20dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:background">@drawable/selectable_bg_new</item>
        <item name="endIcon">@drawable/angle_small_right_1_</item>
    </style>

    <style name="outlined_spinner">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:background">@drawable/custom_spinner_background</item>
        <item name="android:paddingHorizontal" tools:targetApi="26">15dp</item>
        <item name="android:paddingVertical" tools:targetApi="26">18dp</item>
        <item name="android:popupBackground">@drawable/custom_spinner_background_with_padding</item>
        <item name="android:spinnerMode">dropdown</item>
    </style>


    <style name="circular_progress_indicator_home">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:indeterminate">false</item>
        <item name="indicatorColor">?colorOnBackground</item>
        <item name="indicatorSize">65dp</item>
        <item name="thumbTrackGapSize">0dp</item>
        <item name="trackColor">?colorButtonNormal</item>

    </style>

    <style name="simpleCardView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="cardCornerRadius">@dimen/buttonRadius</item>
        <item name="cardBackgroundColor">?cardBackgroundColor</item>
        <item name="strokeWidth">0dp</item>
    </style>

    <style name="floatingActionButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/horizontalSpacing</item>
        <item name="android:backgroundTint">?colorOnTertiary</item>
        <item name="tint">?colorOnBackground</item>
        <item name="android:contentDescription">@string/add</item>
    </style>

    <style name="vertical_divider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">?colorButtonNormal</item>
        <item name="android:layout_marginHorizontal">8dp</item>
    </style>

    <style name="cardStyle_Admin">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:background">?backgroundColor</item>
        <item name="cardCornerRadius">@dimen/buttonRadius</item>
        <item name="cardElevation">1dp</item>
        <item name="cardBackgroundColor">?colorOnTertiary</item>
        <item name="cardUseCompatPadding">true</item>
        <item name="strokeColor">?colorButtonNormal</item>
        <item name="strokeWidth">1dp</item>
    </style>


    <style name="BottomSheetDialogTheme" parent="Theme.Material3.DayNight.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/BottomSheetStyle</item>
    </style>

    <style name="BottomSheetStyle" parent="Widget.Material3.BottomSheet">
        <item name="backgroundTint">@android:color/white</item>
    </style>


    <string name="add">add</string>
